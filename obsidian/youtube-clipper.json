{
	"schemaVersion": "0.1.0",
	"name": "YouTube",
	"behavior": "create",
	"noteContentFormat": "# Video URL\n\n[{{title}}]({{meta:property:og:url}}) â€” [{{schema:@VideoObject:author}}](https://www.youtube.com{{selector:#owner a.ytd-video-owner-renderer?href}}) ({{selector:ytd-player .ytp-time-duration|trim}})\n![{{title}}]({{meta:property:og:url}})\n\n# Description\n\n{{selectorHtml:#inline-expander #snippet-text > #attributed-snippet-text > .yt-core-attributed-string--white-space-pre-wrap|strip_tags:(\"p,strong,em,a\")|strip_attr:(\"href\")|replace:(\"/\\/watch\\?v=/gmi\":\"https\\://www.youtube.com/watch?v=\",\"/^(.*\\S.*)$\\n-{3,}/gmi\":\"$1\\n\\n---\\n\",\"/^-{3,}$/gmi\":\"---\\n\",\"/\\n{3,}/gmi\":\"\\n\\n\",\"/\\n/g\":\"\\n<br>\")|markdown|replace:(\"/^\\s{1,}$/gm\":\"\",\"/^\\\\-/gm\":\"-\",\"/^(.+?) {1,}$/gm\":\"$1\")}}\n",
	"properties": [
		{
			"name": "title",
			"value": "{{schema:@VideoObject:name}}",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "source/web/youtube",
			"type": "multitext"
		},
		{
			"name": "url",
			"value": "{{meta:property:og:url}}",
			"type": "text"
		},
		{
			"name": "channel",
			"value": "{{schema:@VideoObject:author}}",
			"type": "text"
		},
		{
			"name": "channelURL",
			"value": "https://www.youtube.com{{selector:#owner a.ytd-video-owner-renderer?href}}",
			"type": "text"
		},
		{
			"name": "videoID",
			"value": "{{videoId}}",
			"type": "text"
		},
		{
			"name": "related",
			"value": "[[Videos]]",
			"type": "multitext"
		},
		{
			"name": "published",
			"value": "{{schema:@VideoObject:uploadDate|date:\\\"YYYY-MM-DD\\\"}}",
			"type": "date"
		},
		{
			"name": "thumbnailUrl",
			"value": "{{schema:@VideoObject:thumbnailUrl|first}}",
			"type": "text"
		},
		{
			"name": "duration",
			"value": "{{schema:@VideoObject:duration|replace:\\\"PT\\\",\\\"\\\",\\\"S\\\",\\\"\\\"}}",
			"type": "number"
		},
		{
			"name": "durationFormatted",
			"value": "{{selector:ytd-player .ytp-time-duration|trim}}",
			"type": "text"
		},
		{
			"name": "cssclasses",
			"value": "table-col-min-width, wide, img-grid",
			"type": "multitext"
		},
		{
			"name": "created",
			"value": "{{time|date:\\\"YYYY-MM-DDTHH:mm:ssZ\\\"}}",
			"type": "date"
		}
	],
	"triggers": [
		"https://www.youtube.com/watch"
	],
	"noteNameFormat": "Youtube - {{noteName|trim}}",
	"path": "97WebCut/Youtube"
}